{
  "name": "stitch-library",
  "version": "0.0.8",
  "description": "stitch-library React components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "css",
    "*.scss",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "node-sass": "^4.14.1",
    "nwb": "^0.25.2",
    "nwb-sass": "^0.10.2",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.0"
  },
  "author": "Jeanette Silvas",
  "homepage": "https://github.com/jeanettesilvas/stitch-library#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeanettesilvas/stitch-library.git"
  },
  "keywords": [
    "react-component"
  ],
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "bugs": {
    "url": "https://github.com/jeanettesilvas/stitch-library/issues"
  }
}
