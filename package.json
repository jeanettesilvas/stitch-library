{
  "name": "@jeanettesilvas/stitch-library",
  "version": "2.2.0",
  "description": "stitch-library React components",
  "main": "es/src/index.js",
  "types": "es/src/index.d.ts",
  "module": "es/src/index.ds",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "css",
    "*.scss",
    "es",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files --no-demo && tsc && npm run copy && npm run clean:dupes",
    "copy": "cp -R ./es/components ./es/src && cp -R ./es/style ./es/src",
    "clean": "nwb clean-module && nwb clean-demo",
    "clean:dupes": "rm -rf ./es/components && rm -rf ./es/style",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "jest",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "version": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/pro-light-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^27.0.4",
    "nwb": "0.25.x",
    "nwb-sass": "^0.10.2",
    "prettier": "2.3.0",
    "prop-types": "^15.7.2",
    "react": "*",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^10.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^8.2.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "author": "Jeanette Silvas",
  "homepage": "https://github.com/jeanettesilvas/stitch-library#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeanettesilvas/stitch-library.git"
  },
  "keywords": [
    "react-component",
    "component-library"
  ],
  "dependencies": {
    "loader-utils": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/jeanettesilvas/stitch-library/issues"
  },
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
